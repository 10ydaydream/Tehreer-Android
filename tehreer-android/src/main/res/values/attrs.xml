<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Label">
        <attr name="gravity" format="enum">
            <enum name="top" value="0x30"/>
            <enum name="bottom" value="0x50"/>
            <enum name="left" value="0x03"/>
            <enum name="right" value="0x05"/>
            <enum name="center_vertical" value="0x10"/>
            <enum name="center_horizontal" value="0x01"/>
            <enum name="center" value="0x11"/>
        </attr>
        <attr name="maxLines" format="integer" />
        <attr name="shadowRadius" format="dimension" />
        <attr name="shadowDx" format="dimension" />
        <attr name="shadowDy" format="dimension" />
        <attr name="shadowColor" format="color" />
        <attr name="text" format="string" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="truncationMode" format="enum">
            <enum name="none" value="0"/>
            <enum name="word_ellipsis" value="1"/>
            <enum name="character_ellipsis" value="2"/>
        </attr>
        <attr name="truncationPlace" format="enum">
            <enum name="end" value="0"/>
            <enum name="start" value="1"/>
            <enum name="middle" value="2"/>
        </attr>
        <attr name="typeface" format="string" />
    </declare-styleable>
</resources>
